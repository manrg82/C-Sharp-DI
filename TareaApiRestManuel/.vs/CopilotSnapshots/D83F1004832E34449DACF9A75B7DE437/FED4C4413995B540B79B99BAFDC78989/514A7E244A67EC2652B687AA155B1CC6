using Microsoft.AspNetCore.Mvc;

namespace TareaApiRestManuelWeb.Controllers
{
    public class HomeController : Controller
    {
        public IActionResult Index()
        {
            var xml = "<?xml version=\"1.0\" encoding=\"utf-8\"?><home><message>API Objects Demo</message><routes><route>/Objects</route></routes></home>";
            return Content(xml, "application/xml");
        }
    }
}