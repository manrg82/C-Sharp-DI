<Window x:Class="App17102025.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:App17102025"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas Name="Contenedor">
            <Button Canvas.Top="10" Canvas.Left="10" Content="BtnTopLeft"/>
            <Button Canvas.Bottom="10" Canvas.Right="10" Content="BtnBotRight"/>
            <Button Content="BbtnTopRight" Canvas.Top="10" Canvas.Right="10"/>
            <Button Canvas.Bottom="10" Canvas.Left="10" Content="BtnBotLeft"/>
        </Canvas>
        <Canvas Name="Figures">
            <Ellipse Panel.ZIndex="2" Fill="Lime" Canvas.Left="25" Canvas.Top="25" Width="200" Height="200"/>
            <Rectangle Panel.ZIndex="4" Fill="DarkMagenta" Canvas.Left="25" Canvas.Top="25" Width="50" Height="50"/>
            <Rectangle Panel.ZIndex="3" Fill="MediumTurquoise" Canvas.Left="50" Canvas.Top="50" Width="50" Height="50"/>
            <Rectangle Panel.ZIndex="7" Fill="Red" Canvas.Left="75" Canvas.Top="75" Width="50" Height="50"/>
        </Canvas>
    </Grid>
</Window>
